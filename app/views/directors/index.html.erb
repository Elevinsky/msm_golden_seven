<h1> Directors </h1>

<a href="/directors/new_form">Add a New Director </a>

<table>
<tr>
  <th>Image</th>
  <th>Dob</th>
  <th>Name</th>
  <th>Bio</th>
  <th> </th>
</tr>
<% @directors.each do |director| %>
<tr>
  <td> <img src= "<%= director.image_url%>" width="80"> </td>
  <td> <%= director.dob%> </td>
  <td> <%= director.name%> </td>
  <td> <%= director.bio%> </td>
  <td> <a href="/directors/<%= director.id%>">detail</a>,<a href="/directors/<%= director.id%>/edit_form">edit</a>,<a href="/directors/delete_director/<%= director.id%>">delete</a> </td>
<% end %>
</tr>
</table>
